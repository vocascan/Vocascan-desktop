{
  "upload": {
    "type": "json",
    "files": "src/i18n/en/electron.json",
    "deprecate": "file",
    "features": ["array", "filter_untranslated", "plural_postfix_us"]
  },
  "download": {
    "files": {
      "conditions": ["equals: ${file}, electron.json"],
      "output": "src/i18n/${lang}/electron.json"
    }
  }
}
